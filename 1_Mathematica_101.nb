Cell[TextData[{
 "Divide & Conquer: encapsulate your stuff into ",
 StyleBox["Mathematica",
  FontSlant->"Italic"],
 " functions."
}], "Subsection",
 CellChangeTimes->{{3.579863915272234*^9, 3.579863915373168*^9}}],

Cell["\<\
Always ...
1. Localize all local variables (using Module)!
2. Never, ever, use global variables!
3. Use strongly typed arguments and check user input (which is pretty easy as \
patterns are used to specify arguments)!
4. Abort evaluation (and provide a meaningful message) if user input does not \
match the function\[CloseCurlyQuote]s argument signature\
\>", "Text"],

Cell[BoxData[{
 RowBox[{
  RowBox[{"Unprotect", "[", "function", "]"}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"function", "::", "wrngargs"}], "=", 
   "\"\<Provided arguments don't match any definition of function\>\""}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"function", "[", 
    RowBox[{"arg1_Integer", ",", "arg2_List"}], "]"}], ":=", 
   RowBox[{"Module", "[", 
    RowBox[{
     RowBox[{"{", "localVariable", "}"}], ",", "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"localVariable", "=", 
       RowBox[{"arg1", "+", "2"}]}], ";", "\[IndentingNewLine]", 
      RowBox[{"localVariable", "+", "arg2"}]}]}], "\[IndentingNewLine]", 
    "]"}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"function", "[", 
    RowBox[{"arg1_Real", ",", "arg2_List"}], "]"}], ":=", 
   RowBox[{"Module", "[", 
    RowBox[{
     RowBox[{"{", "localVariable", "}"}], ",", "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"localVariable", "=", 
       RowBox[{"arg1", "+", "2"}]}], ";", "\[IndentingNewLine]", 
      RowBox[{"localVariable", "/", "arg2"}]}]}], "\[IndentingNewLine]", 
    "]"}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"function", "[", "___", "]"}], ":=", 
  RowBox[{"(", 
   RowBox[{
    RowBox[{"Message", "[", 
     RowBox[{"function", "::", "wrngargs"}], "]"}], ";", 
    RowBox[{"Abort", "[", "]"}]}], ")"}]}], "\[IndentingNewLine]", 
 RowBox[{
